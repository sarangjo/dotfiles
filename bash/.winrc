# TODO - should the aliases that are specific to WSL or MSYS2 be separated?

# These should work on WSL or MSYS2
alias au17="cd ~/OneDrive\ -\ UW\ Office\ 365/Documents/Senior\ Year/au17"
alias wi18="cd ~/OneDrive\ -\ UW\ Office\ 365/Documents/Senior\ Year/wi18"
alias sp18="cd ~/OneDrive\ -\ UW\ Office\ 365/Documents/Senior\ Year/sp18"
alias onedrive="cd ~/OneDrive/Documents"

# WSL-specific
export DOCKER_HOST=tcp://localhost:2375
export DISPLAY=:0

alias pycharm_linux="~/Downloads/Linux\ Applications/pycharm-community-2017.3.4/bin/pycharm.sh"

function atom() {
    powershell.exe start atom -ArgumentList $1
}

# MSYS2-specific
export PATH=$PATH:/pkgs/winpty/bin

alias wpython="winpty /c/Python36-32/python"
alias wpython2="winpty /c/Python27/python"
