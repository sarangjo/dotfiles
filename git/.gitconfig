[alias]
	s = status
	c = commit -m
	ac = commit -am
	a = add -A
	d = diff
	ps = push
	pl = pull
    co = checkout
[user]
	email = sarangj@msn.com
	name = Sarang Joshi
[core]
	autocrlf = input
	editor = vim
	safecrlf = false
[push]
	default = simple
[pull]
	rebase = true
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
